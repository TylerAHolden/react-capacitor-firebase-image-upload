{
  "name": "react-capacitor-firebase-image-upload",
  "version": "1.4.0",
  "description": "React/Capacitor image upload component for firebase storage",
  "author": "Tyler Holden",
  "license": "Apache-2.0",
  "repository": "TylerAHolden/react-capacitor-firebase-image-upload",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "build-watch": "rollup -c -w",
    "prepublishOnly": "npm run build",
    "start-example": "cd docs && yarn start",
    "deploy-example": "yarn && cd docs && yarn && yarn build",
    "start": "concurrently --kill-others \"yarn build-watch\" \"yarn start-example\""
  },
  "dependencies": {
    "@ionic/react": "^5.4.1",
    "ionicons": "^5.2.3",
    "uuid": "^8.3.1"
  },
  "peerDependencies": {
    "react": ">= 16.8.0 < 2",
    "react-dom": ">= 16.8.0 < 2",
    "styled-components": "^5.1.1"
  },
  "devDependencies": {
    "@types/react": "^16.3.13",
    "@types/react-dom": "^16.0.5",
    "@types/styled-components": "^5.1.4",
    "@types/uuid": "^8.3.0",
    "babel-core": "^6.26.3",
    "concurrently": "^5.3.0",
    "install-peers-cli": "^2.2.0",
    "babel-runtime": "^6.26.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "rollup": "^1.29.0",
    "rollup-plugin-typescript2": "^0.25.3",
    "typescript": "3.8.3"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "image",
    "upload",
    "firebase",
    "react",
    "capacitor",
    "ionic",
    "storage"
  ]
}
