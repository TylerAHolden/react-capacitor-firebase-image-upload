{
  "name": "react-capacitor-firebase-image-upload",
  "version": "2.0.0",
  "description": "React/Capacitor image upload component for firebase storage",
  "author": "Tyler Holden",
  "license": "Apache-2.0",
  "repository": "TylerAHolden/react-capacitor-firebase-image-upload",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "build-watch": "rollup -c -w",
    "prepublishOnly": "npm run build",
    "start-example": "cd demo && yarn start",
    "deploy-example": "yarn build && cd demo && yarn && yarn build",
    "start": "concurrently --kill-others \"yarn build-watch\" \"yarn start-example\""
  },
  "dependencies": {
    "@ionic/react": "^5.4.1",
    "ionicons": "^5.2.3",
    "uuid": "^8.3.1"
  },
  "peerDependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "styled-components": "^5.0.1"
  },
  "devDependencies": {
    "@types/react": "^16.3.13",
    "@types/react-dom": "^16.0.5",
    "@types/styled-components": "^5.1.4",
    "@types/uuid": "^8.3.0",
    "concurrently": "^5.3.0",
    "postcss": "^7.0.32",
    "prepend-file": "^1.3.1",
    "rollup": "^2.3.1",
    "rollup-plugin-postcss": "^3.1.2",
    "rollup-plugin-typescript2": "^0.27.0",
    "ts-node": "^8.8.1",
    "tslib": "^1.11.1",
    "typescript": "3.8.3"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "image",
    "upload",
    "firebase",
    "react",
    "capacitor",
    "ionic",
    "storage"
  ]
}
